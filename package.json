{
  "name": "pyrrho-little-moon-themes",
  "displayName": "Little Moon Color Themes",
  "description": "An interpretation of the Selenized color scheme; simple and readable",
  "version": "0.0.1",
  "publisher": "pyrrho",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Little Moon (dark)",
        "uiTheme": "vs-dark",
