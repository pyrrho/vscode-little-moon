  "homepage": "https://github.com/pyrrho/vscode-little-moon",
  "repository": {
    "type": "git",
    "url": "https://github.com/pyrrho/vscode-little-moon.git"
  },
  "bugs": "https://github.com/pyrrho/vscode-little-moon/issues",
  "packageManager": "pnpm@9.0.0+sha256.bdfc9a7b372b5c462176993e586492603e20da5864d2f8881edc2462482c76fa",
  "scripts": {
    "build": "pnpm dlx @11ty/eleventy",
